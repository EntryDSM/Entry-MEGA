{
  "name": "entry-mega",
  "private": true,
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ]
  },
  "packageManager": "yarn@4.5.1",
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@types/react": "19.0.2",
    "@types/react-dom": "19.0.2",
    "eslint": "^9.14.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --cache --write",
      "eslint --cache --fix --max-warnings=0"
    ]
  },
  "scripts": {
    "format": "prettier --cache --write .",
    "lint": "eslint --cache ."
  },
  "dependencies": {
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-modal": "^3.16.3"
  }
}
